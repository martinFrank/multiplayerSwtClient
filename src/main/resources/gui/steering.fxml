<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" vgap="4" hgap="4">
    <columnConstraints>
        <ColumnConstraints hgrow="NEVER" minWidth="35.0" prefWidth="35.0" />
        <ColumnConstraints hgrow="NEVER" minWidth="35.0" prefWidth="35.0" />
        <ColumnConstraints hgrow="NEVER" minWidth="35.0" prefWidth="35.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="35.0" prefHeight="35.0" vgrow="NEVER" />
        <RowConstraints minHeight="35.0" prefHeight="35.0" vgrow="NEVER" />
        <RowConstraints minHeight="35.0" prefHeight="35.0" vgrow="NEVER" />
    </rowConstraints>
    <children>
        <Button fx:id="steeringNW" text="NW" GridPane.columnIndex="0" GridPane.rowIndex="0" maxWidth="Infinity" maxHeight="Infinity"/>
        <Button fx:id="steeringN" text="N" GridPane.columnIndex="1" GridPane.rowIndex="0" maxWidth="Infinity" maxHeight="Infinity"/>
        <Button fx:id="steeringNE" text="NE" GridPane.columnIndex="2" GridPane.rowIndex="0" maxWidth="Infinity" maxHeight="Infinity"/>
        <Button fx:id="steeringW" text="W" GridPane.columnIndex="0" GridPane.rowIndex="1" maxWidth="Infinity" maxHeight="Infinity"/>
        <Button fx:id="steeringC" text="C" GridPane.columnIndex="1" GridPane.rowIndex="1" maxWidth="Infinity" maxHeight="Infinity"/>
        <Button fx:id="steeringE" text="E" GridPane.columnIndex="2" GridPane.rowIndex="1" maxWidth="Infinity" maxHeight="Infinity"/>
        <Button fx:id="steeringSW" text="SW" GridPane.columnIndex="0" GridPane.rowIndex="2" maxWidth="Infinity" maxHeight="Infinity"/>
        <Button fx:id="steeringS" text="S" GridPane.columnIndex="1" GridPane.rowIndex="2" maxWidth="Infinity" maxHeight="Infinity"/>
        <Button fx:id="steeringSE" text="SE" GridPane.columnIndex="2" GridPane.rowIndex="2" maxWidth="Infinity" maxHeight="Infinity"/>
    </children>
</GridPane>
